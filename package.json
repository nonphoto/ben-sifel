{
  "name": "ben-sifel",
  "version": "1.0.0",
  "description": "A portfolio site for Ben Sifel.",
  "main": "main.js",
  "scripts": {
    "start": "npm run build && npm run deploy && npm run alias",
    "build": "webpack -p --config webpack.config.js",
