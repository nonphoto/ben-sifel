  "dependencies": {
    "animejs": "^3.0.1",
    "simplex-noise-esm": "^2.5.0-esm.0",
    "three": "^0.90.0"
  },
  "devDependencies": {
    "@pika/web": "^0.4.2",
    "now": "^12.1.5"
  }
}
